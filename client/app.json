{
  "expo": {
    "name": "I Servitori Dell'Arte",
    "slug": "iservitoridellarte",
    "version": "1.5.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/servitori-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#161616"
    },
    "updates": {
      "enabled": true,
      "useClassicUpdates": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.iservitoridellarte"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iservitoridellarte",
      "versionCode": 4
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@stripe/stripe-react-native",
        {
          "enableGooglePay": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "fdefbf48-cd51-4ab4-b8e3-fbe9d8de0c83"
      }
    }
  }
}
